<%- include('layouts/layout') %>
        <div class="profile">
            <img src="data:image/jpeg;base64," alt="Profile Picture">
            <h2><%= user.userName%></h2>
            <p><%= user.email%></p>
        </div>
        <h2>Read Your all Blogs</h2>
        <div id="blog-container">
            <% blogs.forEach( blog => { %>
                <div class="blog">
                    <div class="blog-text">
                        <h2><%= blog.title %></h2>
                        <h4><%= blog.author %></h4>
                        <% if(blog.content.length > 100) { %>
                            <p><%= blog.content.substring(0, 100) %></p>
                        <% } else { %>
                            <p><%= blog.content %></p>
                        <% } %>
                        <a href="/user/blog/<%= blog.id %>">Read more...</a>
                    </div>
                </div>
            <% }); %>
        </div> 
    </body>
</html>